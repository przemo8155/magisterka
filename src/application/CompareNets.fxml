<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="application.CompareNetsController">
   <children>
      <Pane fx:id="mainPane" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label fx:id="titleLabel" layoutX="219.0" layoutY="14.0" text="COMPARE NETS">
               <font>
                  <Font name="Lucida Bright Demibold" size="20.0" />
               </font>
            </Label>
            <Label fx:id="net1Label" layoutX="286.0" layoutY="53.0" text="Net 1" />
            <Label fx:id="net2Label" layoutX="286.0" layoutY="192.0" text="Net 2" />
            <Button fx:id="selectCurrently1Button" layoutX="219.0" layoutY="77.0" mnemonicParsing="false" onAction="#selectCurrently1Button_OnAction" text="Select currently" />
            <Button fx:id="selectNet1Button" layoutX="330.0" layoutY="77.0" mnemonicParsing="false" onAction="#selectNet1Button_OnAction" text="Select net from file" />
            <Button fx:id="compareButton" layoutX="242.0" layoutY="344.0" mnemonicParsing="false" onAction="#compareButton_OnAction" text="Compare" />
            <Button fx:id="closeButton" layoutX="330.0" layoutY="344.0" mnemonicParsing="false" text="Close" />
            <Button fx:id="selectCurrently2Button" layoutX="193.0" layoutY="262.0" mnemonicParsing="false" onAction="#selectCurrently2Button_OnAction" text="Select currently" />
            <Button fx:id="selectNet2Button" layoutX="317.0" layoutY="262.0" mnemonicParsing="false" onAction="#selectNet2Button_OnAction" text="Select net from file" />
            <TextField fx:id="file1TextField" layoutX="241.0" layoutY="116.0" />
            <TextField fx:id="file2TextField" layoutX="232.0" layoutY="298.0" />
         </children>
      </Pane>
   </children>
</AnchorPane>
