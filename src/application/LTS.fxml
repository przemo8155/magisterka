<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="application.LTSController">
   <children>
		<ScrollPane fx:id="scrollMainPane" layoutX="6.0" prefHeight="600.0" prefWidth="800.0">
			<content>
				<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
			</content>
		</ScrollPane>
		<Pane fx:id="mainPane" prefHeight="600.0" prefWidth="800.0">
			<children>
				<Button fx:id="closeButton" layoutX="357.0" layoutY="561.0" mnemonicParsing="false" onAction="#closeButton_OnAction" text="Hide preview" />
            <Label fx:id="pathInfoLabel" layoutX="14.0" layoutY="23.0" text="PATH:">
               <font>
                  <Font name="Georgia" size="14.0" />
               </font>
               <effect>
                  <InnerShadow />
               </effect></Label>
            <Label fx:id="pathLabel" layoutX="14.0" layoutY="60.0">
               <font>
                  <Font size="14.0" />
               </font>
               <effect>
                  <InnerShadow color="#1700ae" />
               </effect></Label>
            <Label fx:id="ltsPreviewLabel" layoutX="345.0" layoutY="16.0" text="LTS Preview">
               <font>
                  <Font name="Gadugi" size="21.0" />
               </font>
               <effect>
                  <InnerShadow color="#062bff" radius="4.75" width="0.0" />
               </effect>
            </Label>

			</children>
		</Pane>
	</children>
</AnchorPane>
